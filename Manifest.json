{
  "manifest_version": 2,
  "name": "YouTube Studio Data Extractor",
  "version": "1.0",
  "description": "Extracts data from YouTube Studio.",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["https://studio.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["tabs", "activeTab"]
}
